    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) CalcTwoPic_x64",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/test/x64/CalcTwoPic_x64",
            "args": ["--pic1=../res/pic1.bmp", "--pic2=../res/pic1.png", "--n=10"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/test/x64",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) - OnlyAuth_x64",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/test/x64/OnlyAuth_x64",
            // "args": [],
            "args": ["--no-button", 
                "--no-laser", 
                "--no-led",
                // "--no-lcd", 
                "--mid-save=./mid-save",
                "--resolution-index=15", "--ROI-x=50", "--ROI-y=50",
                "--ROI-w=800", "--ROI-h=600",
                "--exposion-time=6000",
                "--laser-current=300",
                "--lcd-width=100",
                "--lcd-height=100"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/test/x64",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            // "preLaunchTask": "C/C++: g++ build active file",
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}